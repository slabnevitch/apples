extends _misc/main

block title
	| Accessories

block content
	section.breadcrumbs
		.wrapper
			ul.appl-breadcrumbs
				li.last
					a(href="#") Аксесуари

	section.products
		.wrapper
			.products-sidebar
				- var menu = ['Mac', 'iPad', 'iPhone', 'Watch', 'TV', 'AirPods', 'HomePod'];
				ul
					each item in menu
						li
							a(href="#")= item

			.products-content
				.wrapper-wide.product-cards
					- for (var i=0; i < 9; i++)
						a(href="#").product-card
							.product-card__img
								img(src="img/accessories-page/product.jpg", alt="alt")
							.product-card__name Air Pods 2 Wireless Charging
							.product-card__price 4 199 грн



